<% provide(:title, 'スキル登録') %>
<h1>スキル登録</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@skill) do |s| %>
      <%= render 'shared/error_messages' %>

      <%= s.label :name, "曲名" %>
      <%= s.collection_select :music_id, Prefecture.all, :id, :name %>

      <%= s.comment :comment %>
      <%= s.text_field :comment %>

      難易度

      達成率

      フルコンボ

      <%= f.submit "変更", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
